module Grammar.Skel where

-- Haskell module generated by the BNF converter

import Grammar.Abs
import Grammar.ErrM
type Result = Err String

failure :: Show a => a -> Result
failure x = Bad $ "Undefined case: " ++ show x

transIdent :: Ident -> Result
transIdent x = case x of
  Ident string -> failure x
transProg :: Show a => Prog a -> Result
transProg x = case x of
  Program _ topdefvars topdefs blok -> failure x
transTopDefVar :: Show a => TopDefVar a -> Result
transTopDefVar x = case x of
  VarDef _ type_ ident -> failure x
transTopDef :: Show a => TopDef a -> Result
transTopDef x = case x of
  FnDef _ type_ ident args blok -> failure x
transArg :: Show a => Arg a -> Result
transArg x = case x of
  Argcopy _ type_ ident -> failure x
  Argref _ type_ ident -> failure x
transBlok :: Show a => Blok a -> Result
transBlok x = case x of
  Block _ stmts -> failure x
transStmt :: Show a => Stmt a -> Result
transStmt x = case x of
  Empty _ -> failure x
  Decl _ topdefvar -> failure x
  DeclFun _ topdef -> failure x
  Ass _ type_ ident expr -> failure x
  Incr _ type_ ident -> failure x
  Decr _ type_ ident -> failure x
  Ret _ expr -> failure x
  Retv _ -> failure x
  Cond _ expr stmts -> failure x
  CondElse _ expr stmts1 stmts2 -> failure x
  While _ expr stmts -> failure x
  WhileSuspended _ expr stmts -> failure x
  WhileContinued _ expr stmts -> failure x
  SExp _ expr -> failure x
  Continue _ -> failure x
  Break _ -> failure x
  Print _ expr -> failure x
transType :: Show a => Type a -> Result
transType x = case x of
  Int _ -> failure x
  Str _ -> failure x
  Bool _ -> failure x
  Void _ -> failure x
transExpr :: Show a => Expr a -> Result
transExpr x = case x of
  EVar _ type_ ident -> failure x
  ELitInt _ integer -> failure x
  ELitTrue _ -> failure x
  ELitFalse _ -> failure x
  EApp _ type_ ident exprs -> failure x
  EString _ string -> failure x
  Neg _ expr -> failure x
  Not _ expr -> failure x
  EMul _ expr1 mulop expr2 -> failure x
  EAdd _ expr1 addop expr2 -> failure x
  ERel _ expr1 relop expr2 -> failure x
  EAnd _ expr1 expr2 -> failure x
  EOr _ expr1 expr2 -> failure x
transAddOp :: Show a => AddOp a -> Result
transAddOp x = case x of
  Plus _ -> failure x
  Minus _ -> failure x
transMulOp :: Show a => MulOp a -> Result
transMulOp x = case x of
  Times _ -> failure x
  Div _ -> failure x
  Mod _ -> failure x
transRelOp :: Show a => RelOp a -> Result
transRelOp x = case x of
  LTH _ -> failure x
  LE _ -> failure x
  GTH _ -> failure x
  GE _ -> failure x
  EQU _ -> failure x
  NE _ -> failure x

